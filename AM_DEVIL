def pilih():
    unikers = raw_input('\x1b[1;33mPilih \x1b[1;31m:\x1b[1;37m ')
    if unikers == '':
        print '\x1b[1;97m{\x1b[1;91m!\x1b[1;97m}\x1b[1;97m Content That Is True Dear!'
        pilih()
    elif unikers == '1':
        super()
    elif unikers == '0':
        jalan('Token Removed')
        os.system('rm -rf login.txt')
        keluar()
    else:
        print '\x1b[1;97m{\x1b[1;91m!\x1b[1;97m}\x1b[1;97m The contents are true dear !'
        pilih()


